{
  "version": "1.0.0",
  "llm": {
    "endpoint": "http://host.docker.internal:1234/v1/chat/completions",
    "model": "qwen/qwen3-8b",
    "temperature": 0.7
  },
  "agent": {
    "soft_limit": 2048,
    "hard_limit": 4096,
    "max_iterate": 10,
    "default_status": "thinking",
    "prompt": {
      "system": "You are a ai assistant.",
      "thinking": "You are thinking about the next steps.",
      "paging": "You are paging through the data.",
      "evaluating": "You are evaluating the current state.",
      "executing": "You are executing the plan."
    }
  }
}