{
  "version": "1.0.0",
  "llm": {
    "endpoint": "http://host.docker.internal:1234/v1/chat/completions",
    "model": "qwen/qwen3-8b",
    "temperature": 0.7
  },
  "agent": {
    "soft_limit": 4096,
    "hard_limit": 8192,
    "max_iterate": 9223372036854775807,
    "default_status": "thinking",
    "prompt": {
      "system": {
        "output_format": "JSON",
        "task": "Write a endless story",
        "output_example1": {
          "working_memory_add": {
            "character_setting_bob": "male, 30 years old, cautious, enjoys reading"
          }
        },
        "output_example2": {
          "working_memory_remove": "character_setting_alice"
        }
      },
      "thinking": {},
      "paging": {},
      "evaluating": {},
      "executing": {}
    }
  }
}