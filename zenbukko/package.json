{
  "name": "zenbukko",
  "version": "1.0.0",
  "description": "NNN Lecture Downloader & Processor CLI Tool",
  "main": "dist/index.js",
  "bin": {
    "zenbukko": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node --watch src/index.ts",
    "setup:whisper": "ts-node src/scripts/setup-whisper.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nnn",
    "education",
    "downloader",
    "cli",
    "typescript",
    "functional-programming"
