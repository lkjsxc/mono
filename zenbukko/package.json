  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.7.7",
    "cheerio": "^1.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fp-ts": "^2.16.9",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^23.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=22.0.0"
